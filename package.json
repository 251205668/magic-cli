{
  "name": "@vbs/magic-cli",
  "version": "1.0.15",
  "private": true,
  "packageManager": "pnpm@7.27.1",
  "scripts": {
    "start": "zr dev && cd packages/core && npm link",
    "dev": "pnpm -r --parallel --filter=./packages/* run stub",
    "build": "pnpm -r --parallel --filter=./packages/* run build",
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm run -C docs build",
    "lint": "eslint --cache .",
    "lint:fix": "zr lint --fix",
    "prepare": "husky install",
    "release": "bumpp package.json packages/**/package.json",
    "deploy": "zr build && zr docs:build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/chalk": "2.2.0",
    "@types/clear": "^0.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^8.1.0",
    "@types/node": "^18.6.4",
    "@types/ora": "^3.2.0",
    "@types/root-check": "^1.0.0",
    "@types/semver": "^7.3.12",
    "@types/semver-sort": "^0.0.1",
    "@vbs/eslint-config": "^1.0.0",
    "@vbs/magic-cli-core": "workspace:*",
    "@vbs/magic-cli-init": "workspace:*",
    "@vbs/magic-cli-models": "workspace:*",
    "@vbs/magic-cli-templates": "workspace:*",
    "@vbs/magic-cli-utils": "workspace:*",
    "axios": "^0.27.2",
    "bumpp": "^8.2.1",
    "chalk": "4.1.1",
    "clear": "^0.1.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.34.0",
    "execa": "^6.1.0",
    "figlet": "^1.5.2",
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "gradient-string": "^2.0.1",
    "husky": "^7.0.4",
    "inquirer": "^8.1.0",
    "lint-staged": "^10.5.4",
    "npminstall": "^6.5.0",
    "ora": "^6.1.2",
    "pkg-dir": "^5.0.0",
    "root-check": "^2.0.0",
    "semver": "^7.3.7",
    "semver-sort": "^1.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "unbuild": "^0.8.8",
    "za-zi": "^0.0.10"
  },
  "lint-staged": {
    "*": "npm run lint"
  },
  "eslintConfig": {
    "extends": "@vbs"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
